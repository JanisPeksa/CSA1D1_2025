import java.util.Scanner;

public class main{
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);


        int n = sc.nextInt();
        int m = sc.nextInt();


        int[] A = new int[n];
        int[] B = new int[m];

        for (int i = 0; i < n; i++) A[i] = sc.nextInt();
        for (int i = 0; i < m; i++) B[i] = sc.nextInt();


        int[] C = new int[n + m];
        int i = 0, j = 0, k = 0;

        while (i < n && j < m) {
            if (A[i] < B[j]) {
                C[k++] = A[i++];
            } else {
                C[k++] = B[j++];
            }
        }


        while (i < n) C[k++] = A[i++];
        while (j < m) C[k++] = B[j++];


        for (int x = 0; x < C.length; x++) {
            System.out.print(C[x] + " ");
        }
    }
}
