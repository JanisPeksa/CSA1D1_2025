
import java.util.Scanner;

public class Main {
    public static void main(String[] args) {

        Scanner sc = new Scanner(System.in);

        System.out.print("Enter any password: ");
        String password = sc.nextLine();
        boolean hasLetter = false ,hasDigits = false, hasSymbol = false;

        for(int i = 0 ; i < password.length() ; i++){
            char ch = password.charAt(i);
            if(Character.isDigit(ch)) hasDigits  = true;
            else if(Character.isLetter(ch))   hasLetter = true;
            else hasSymbol =true;

        }

        if(password.length() < 8){
            System.out.println("password is too short");

        }
        else if(hasLetter && !hasDigits && !hasSymbol){
            System.out.println("password is to weak");

        }
        else if (hasLetter && hasDigits && !hasSymbol){
            System.out.println("password is okay");

        }
        else if (password.length() >= 10 && hasLetter && hasDigits && hasSymbol){
            System.out.println("password is strong");

        }
        else{
            System.out.println("weak password");
        }

    }
}
