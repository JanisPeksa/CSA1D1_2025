import java.util.Scanner;

class Main {
    public static void main(String[] args) {

        Scanner sc = new Scanner(System.in);
        System.out.println("Enter number:");

        int i = sc.nextInt();
        i = 0;


        for (i=1; i<=10; i++) {
           System.out.println(i + "*" + i + "=" + i*i);
           i++;
       }
    }
}
