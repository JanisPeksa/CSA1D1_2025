    public static void main(String[] args) {
       int[][] arr1 = new int[10][10];
       int[][] arr2 = new int[10][10];
       int[][] arr3 = new int[10][];
       int[][] arr4 = new int[10][10];
       int[][] arr5 = new int[10][10];
    }
}
