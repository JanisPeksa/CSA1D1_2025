public class Main {
    public static void main(String[] args) {
        System.out.println(" ");
        System.out.println(" ");
        System.out.println("__");
        System.out.println("_ =");
        System.out.println(" ");
        System.out.println(" ");
    }
}
n24
public class Main {
    public static void main(String[] args) {
        int num = 1;
        for (int i = 0; i<5; i++) {
            num*= 2;
            System.out.println(num + " ");
        }
        for (int i = 0 ; i <4; i++) {
            num /= 2;
            if (i < 3){
                System.out.println(num + " ");
            }else{
                System.out.println(num);
            }
        }

        System.out.println();
    }

}
