public class Main {
    public static void main(String[] args) {
        int x = 2;
        if ( x > 10) {
            System.out.println(" A ");
        }
        else if(x > 3){
            System.out.println(" B ");
        }
        else{
            System.out.println(" C ");
        }
    }
}



