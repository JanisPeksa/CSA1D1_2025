public class Main {
    public static void main(String[] args) {
        int x = 5; // value of x
        System.out.println(x);
        System.out.println("x = x + x + 2 => x = "+ x +" + "+ x +" + 2");
        x = x + x + 2; // value of x
        System.out.println(x);
        System.out.println("x = x + x + 1 => x = "+ x +" + "+ x +" + 1");
        x = x + x + 1; // value of x
        System.out.println(x);
