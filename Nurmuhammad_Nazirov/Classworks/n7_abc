public class n7_abc {
    public static void main(String[] args) {
        int x = 4;
        if ( x > 10) {
            System.out.println(" A ");
        }
        else if (x > 3)
        {
            System.out.println(" B ");
        }
        else{
            System.out.println(" C ");
        }
    }
}
