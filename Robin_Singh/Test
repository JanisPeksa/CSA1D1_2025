import java.util.Arrays;
import java.util.Scanner;

public class Main{

    public static void task_1(){
//      Consecutive Increasing Sequence Length




//        k = k % n;
//        for (int j = 0; j < k; j = j + 1) {
//            int last = nums[n - 1];
//            for (int i = n - 1; i > 0; i = i - 1) {
//                nums[i] = nums[i - 1];
//            }
//
//            nums[0] = last;
//        }
//        System.out.print("Rotated Array: ");
//        for (int i = 0; i < n; i = i + 1) {
//            System.out.print(nums[i] + " ");
//        }

        Scanner input = new Scanner(System.in);

        System.out.print("Enter the length of the Array : ");

        int length = input.nextInt();
        int[] array = new int[length];
        int seqLength = 1;


        for (int i = 0; i < length; i++){
            System.out.print("Enter a Number : ");
            array[i] = input.nextInt();
        }

        for (int i = 0; i < array.length; i++){
            int currentElement = array[i];
            int count = 1;

            for(int j = i + 1; j < array.length; j++){
                if (array[j] == currentElement + count ){
                    count++;
                }else {
                    break;
                }
            }

            if (count > seqLength){
                seqLength = count;
            }

        }

        System.out.println("So the Consecutive Increasing Sequence Length is " + seqLength);

        input.close();

    }


    public static void task_2(){


//        Shift Array Right by K Steps (Manual)



        Scanner input = new Scanner(System.in);

        System.out.print("Enter the length of the Array : ");

        int length = input.nextInt();
        int[] array = new int[length];


        for (int i = 0; i < length; i++){
            System.out.print("Enter a Number : ");
            array[i] = input.nextInt();
        }

        System.out.print("Enter the value of k : ");
        int k = input.nextInt();

        int[] newArr = array.clone();

        for (int i = 0; i < array.length; i++){
            int ele = array[i];

            if (i + k > array.length - 1){
                newArr[i + k - array.length] = ele;
            } else {
                newArr[i + k] = ele;
            }




        }


        for (int p = 0; p < newArr.length; p++){
            System.out.println(newArr[p]);
        }




    }


    public static void task_3(){

        Scanner input = new Scanner(System.in);

        System.out.print("Enter the length of the 1st Array : ");

        int length1 = input.nextInt();
        int[] array1 = new int[length1];
        System.out.print("Enter the length of the 2nd Array : ");

        int length2 = input.nextInt();
        int[] array2 = new int[length2];
        int[] newArray = new int[length2 + length1];



        for (int i = 0; i < length1; i++){
            System.out.print("Enter a Number for 1st Array : ");
            int val = input.nextInt();
            array1[i] = val;
            newArray[i] = val;
        }

        for (int i = 0; i < length2; i++){
            System.out.print("Enter a Number for 2nd Array : ");
            int val = input.nextInt();
            array2[i] = val;
            newArray[i + length1] = val;
        }

        Arrays.sort(newArray);

        for (int i = 0; i < newArray.length; i++){
            System.out.print(newArray[i] + " ");
        }

        // array1
        // array2



        input.close();

    }

    public static void main(String[] args) {
//        task_1();
//        task_2();
        task_3();
    }
}
